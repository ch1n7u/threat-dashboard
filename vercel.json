{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/api/lookup", "destination": "/api/lookup.py" },
    { "source": "/static/(.*)", "destination": "/static/$1" }
  ],
  "headers": [
    {
      "source": "/api/lookup",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET" }
      ]
    }
  ]
}