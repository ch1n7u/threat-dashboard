{
  "version": 2,
  "rewrites": [
    { 
      "source": "/api/lookup", 
      "destination": "/api/lookup.py",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    { "source": "/static/(.*)", "destination": "/static/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}